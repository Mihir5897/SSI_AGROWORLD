    <style name="SpinnerTheme" parent="Widget.AppCompat.Spinner.DropDown">
        <item name="android:background">@drawable/bg_spinner</item>
        <item name="android:textStyle">bold</item>
        <item name="fontFamily">@font/poppins_bold</item>
        <item name="android:textColor">#020334</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_marginLeft">30dp</item>
        <item name="android:layout_marginRight">30dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:dropDownVerticalOffset">35dp</item>
        <item name="android:elevation">0dp</item>
        <item name="android:popupElevation">0dp</item>
        <item name="android:spinnerMode">dropdown</item>
    </style>

    <style name="TextInputLayoutHeading">
        <item name="android:layout_marginLeft">30dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">25dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="fontFamily">@font/poppins</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textAlignment">center</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#020334</item>

    </style>

    <style name="TextInputLayoutStyle" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense">
        <item name="android:layout_marginLeft">30dp</item>
        <item name="android:layout_marginRight">30dp</item>
        <item name="boxStrokeColor">#e1e1e1</item>
        <item name="boxStrokeWidth">1dp</item>
        <item name="boxStrokeWidthFocused">1dp</item>
        <item name="hintEnabled">false</item>
        <item name="helperTextTextAppearance">@style/TextInputLayoutHelperText</item>
        <item name="errorTextAppearance">@style/TextInputLayoutErrorText</item>
        <item name="errorIconDrawable">@drawable/ic_end_error</item>
        <item name="endIconMode">clear_text</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="TextInputEditTextStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textColor">#020334</item>
        <item name="hintTextColor">#B4B4C3</item>
        <item name="android:textSize">14sp</item>
    </style>


    <style name="TextInputLayoutHelperText" parent="TextAppearance.Design.HelperText">
        <item name="android:fontFamily">@font/poppins</item>
    </style>

    <style name="TextInputLayoutErrorText" parent="TextAppearance.Design.Error">
